{
    "request": {
        "method": "GET",
        "urlPattern": "/reports/.*"
    },
    "response" : {
        "status" : 200,
        "bodyFileName" : "reports/{{request.pathSegments.[1]}}.html",
        "headers" : {
            "Access-Control-Allow-Origin" : "*",
            "Access-Control-Allow-Methods" : "*",
            "Access-Control-Allow-Headers": "Accept, Content-Type, Content-Encoding, Server, Transfer-Encoding"
        }
    }
}
